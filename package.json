{
  "name": "gh-label",
  "version": "0.3.0",
  "description": "Simple GUI for editing GitHub labels",
  "type": "module",
  "packageManager": "pnpm@8.15.2",
  "private": true,
  "lint-staged": {
    "*.{?(c|m)@(j|t)s,css,vue,md,json,yaml}": "prettier --write"
  },
  "scripts": {
    "build": "cargo tauri build",
    "dev": "cargo tauri dev",
    "format": "prettier . --write && cargo fmt",
    "format-check": "prettier . --check",
    "lint": "eslint . --config eslint.config.js --cache",
    "prepare": "husky",
    "vite:dev": "vite",
    "vite:build": "vite build"
  },
  "dependencies": {
    "@manatsu/tauri-plugin": "^0.4.12",
    "@tauri-apps/api": "^1.5.3",
    "@vueuse/core": "^10.9.0",
    "manatsu": "^0.4.12",
    "pinia": "^2.1.7",
    "vue": "^3.4.21"
  },
  "devDependencies": {
    "@manatsu/sass": "^0.4.12",
    "@tb-dev/eslint-config": "^2.1.2",
    "@tb-dev/utility-types": "^1.2.0",
    "@types/node": "^20.11.24",
    "@vitejs/plugin-vue": "^5.0.4",
    "eslint": "^8.57.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "prettier-plugin-css-order": "^2.0.1",
    "sass": "^1.71.1",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vite": "^5.1.4",
    "vite-plugin-vue-devtools": "^7.0.16"
  }
}
