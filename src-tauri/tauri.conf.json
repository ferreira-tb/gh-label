{
  "productName": "gh-label",
  "version": "0.6.0",
  "identifier": "tbdev.ghlabel",
  "build": {
    "beforeDevCommand": "pnpm run vite:dev",
    "beforeBuildCommand": "pnpm run vite:build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "app": {
    "withGlobalTauri": false,
    "windows": [
      {
        "label": "main",
        "title": "gh-label",
        "width": 800,
        "height": 600,
        "visible": false,
        "fullscreen": false,
        "resizable": false
      }
    ],
    "security": {
      "csp": {
        "default-src": "'self'",
        "connect-src": "ipc: http://ipc.localhost",
        "img-src": "'self' blob: asset: http://asset.localhost",
        "style-src": "'self' 'unsafe-inline'"
      }
    }
  }
}