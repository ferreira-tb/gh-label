{
  "build": {
    "beforeDevCommand": "pnpm run vite:dev",
    "beforeBuildCommand": "pnpm run vite:build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1421"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "productName": "gh-label",
  "version": "0.5.0",
  "identifier": "tbdev.ghlabel",
  "plugins": {},
  "app": {
    "withGlobalTauri": false,
    "windows": [
      {
        "label": "main",
        "title": "gh-label",
        "width": 800,
        "height": 600,
        "visible": false,
        "fullscreen": false,
        "resizable": false
      }
    ],
    "security": {
      "csp": null
    }
  }
}
